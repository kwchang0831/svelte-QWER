{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "endOfLine": "auto",
  "trailingComma": "all",
  "printWidth": 120,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
